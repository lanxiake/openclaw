# Copy to .env and fill with your Twilio credentials
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your_auth_token_here
# Must be a WhatsApp-enabled Twilio number, prefixed with whatsapp:
TWILIO_WHATSAPP_FROM=whatsapp:+17343367101

# ==================== PostgreSQL Database Configuration ====================
# Connection string format: postgresql://user:password@host:port/database
# 默认端口: 22001 (非标准端口)
DATABASE_URL=postgresql://openclaw_admin:Oc@2026!Pg#Secure@localhost:22001/openclaw_prod
# Maximum number of connections in the pool (default: 10)
DATABASE_MAX_CONNECTIONS=10
# Connection timeout in milliseconds (default: 10000)
DATABASE_CONNECTION_TIMEOUT_MS=10000
# Idle timeout in milliseconds (default: 300000 = 5 minutes)
DATABASE_IDLE_TIMEOUT_MS=300000
# SSL mode: false, true, require, prefer (default: false)
DATABASE_SSL=false

# ==================== JWT Configuration ====================
# Secret key for signing JWT tokens (should be a secure random string, at least 32 characters)
JWT_SECRET=Oc@2026!JwtSecret#32CharMinimumRequired
# Access token expiration (default: 15m)
JWT_ACCESS_EXPIRES_IN=15m
# Refresh token expiration (default: 7d)
JWT_REFRESH_EXPIRES_IN=7d

# ==================== Redis Configuration ====================
# Redis connection URL (默认端口: 22002)
REDIS_URL=redis://:Oc@2026!Rd#Secure@localhost:22002/0
# Alternative: individual settings
# REDIS_HOST=localhost
# REDIS_PORT=22002
# REDIS_PASSWORD=Oc@2026!Rd#Secure
# REDIS_DB=0

# ==================== MinIO Configuration (Object Storage) ====================
# 默认端口: API 22003, Console 22004
MINIO_ENDPOINT=localhost
MINIO_PORT=22003
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=openclaw_minio
MINIO_SECRET_KEY=Oc@2026!Mn#Secure

# ==================== Milvus Configuration (Vector Database) ====================
# Used for knowledge memory and semantic search
# 默认端口: 22005
MILVUS_ADDRESS=localhost:22005
MILVUS_TOKEN=
MILVUS_DATABASE=default

# ==================== Neo4j Configuration (Graph Database) ====================
# Used for knowledge graph
# 默认端口: HTTP 22007, Bolt 22008
NEO4J_URI=bolt://localhost:22008
NEO4J_USER=neo4j_admin
NEO4J_PASSWORD=Oc@2026!N4j#Secure

# ==================== Embedding Model Configuration ====================
# Provider: openai, gemini, local
EMBEDDING_PROVIDER=openai
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIMENSIONS=1536
# EMBEDDING_API_KEY=your_api_key
# EMBEDDING_BASE_URL=https://api.openai.com/v1

# ==================== Mem0 Configuration (Memory Service) ====================
# MEM0_API_KEY=your_mem0_api_key
# MEM0_BASE_URL=https://api.mem0.ai

