{
  "name": "openclaw-assistant-windows",
  "version": "0.1.0",
  "description": "OpenClaw AI Assistant for Windows",
  "main": "out/main/index.js",
  "author": {
    "name": "OpenClaw",
    "email": "support@openclaw.ai"
  },
  "license": "MIT",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "package": "pnpm build && electron-builder --config electron-builder.json",
    "package:win": "pnpm build && electron-builder --win --config electron-builder.json",
    "package:win:x64": "pnpm build && electron-builder --win --x64 --config electron-builder.json",
    "package:win:ia32": "pnpm build && electron-builder --win --ia32 --config electron-builder.json",
    "package:nsis": "pnpm build && electron-builder --win nsis --x64 --ia32 --config electron-builder.json",
    "package:nsis:x64": "pnpm build && electron-builder --win nsis --x64 --config electron-builder.json",
    "package:portable": "pnpm build && node scripts/package-portable.js",
    "package:zip": "pnpm build && electron-builder --win zip --x64 --config electron-builder.json",
    "package:dir": "pnpm build && electron-builder --dir --config electron-builder.json",
    "generate:icon": "npx tsx scripts/generate-icon.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf out release"
  },
  "dependencies": {
    "electron-updater": "^6.1.7",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "jsdom": "^28.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^4.0.18"
  }
}
